{
    "package": {
        "package_name": "net.intellivoid.socialvoid_rpc",
        "name": "SocialvoidRPC Server",
        "version": "1.0.0.0",
        "author": "Zi Xing Narrakas",
        "organization": "Intellivoid",
        "description": "The Socialvoid RPC Server service",
        "url": "https://github.com/intellivoid/socialvoidlib",
        "dependencies": [
            {
                "package": "net.intellivoid.socialvoidlib",
                "version": "latest",
                "source": "default@github/intellivoid/SocialvoidLib",
                "required": false
            },
            {
                "package": "net.intellivoid.kimchi_rpc",
                "version": "latest",
                "source": "default@github/intellivoid/KimchiRPC",
                "required": true
            }
        ],
        "configuration": {
            "autoload_method": "generated_spl",
            "main": {
                "execution_point": "main",
                "create_symlink": false,
                "name": null
            },
            "post_installation": [],
            "pre_installation": []
        }
    },
    "components": [
        {
            "required": true,
            "file": "SocialvoidRPC.php"
        },
        {
            "required": true,
            "file": "Methods/Session/GetSession.php"
        },
        {
            "required": true,
            "file": "Methods/Session/CreateSession.php"
        },
        {
            "required": true,
            "file": "Methods/Session/Register.php"
        },
        {
            "required": true,
            "file": "Methods/Session/AuthenticateUser.php"
        },
        {
            "required": true,
            "file": "Methods/Session/Logout.php"
        },
        {
            "required": true,
            "file": "Methods/Network/ResolvePeer.php"
        },
        {
            "required": true,
            "file": "Methods/Network/GetMe.php"
        }
    ],
    "files": [
        "package.json",
        "main",
        "rpc_worker"
    ]
}